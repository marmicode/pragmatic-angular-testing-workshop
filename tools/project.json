{
  "name": "tools",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "tools",
  "targets": {
    "cascade-changes": {
      "command": "node --experimental-strip-types {projectRoot}/cascade-changes.ts"
    },
    "clone-app": {
      "command": "{projectRoot}/clone-app.sh"
    },
    "clone-changes": {
      "command": "{projectRoot}/clone-changes.sh"
    },
    "downgrade-to-jest": {
      "command": "node --experimental-strip-types {projectRoot}/downgrade-to-jest.ts"
    },
    "sanity-check": {
      "command": "node --experimental-strip-types {projectRoot}/sanity-check.ts"
    }
  }
}
